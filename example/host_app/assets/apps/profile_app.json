{
  "name": "ProfileApp",
  "version": "1.0.0",
  "metadata": {
    "title": "Profile",
    "description": "User profile card"
  },
  "states": [
    {"key": "name", "type": "String", "defaultValue": "John Doe"},
    {"key": "role", "type": "String", "defaultValue": "Flutter Developer"},
    {"key": "email", "type": "String", "defaultValue": "john@example.com"},
    {"key": "location", "type": "String", "defaultValue": "San Francisco, CA"},
    {"key": "followers", "type": "int", "defaultValue": 1284},
    {"key": "following", "type": "int", "defaultValue": 567}
  ],
  "actions": [
    {"name": "follow"},
    {"name": "message"},
    {"name": "share"}
  ],
  "root": {
    "type": "Scaffold",
    "appBar": {
      "type": "AppBar",
      "title": "Profile",
      "backgroundColor": {"_color": 4288423856},
      "foregroundColor": {"_color": 4294967295}
    },
    "body": {
      "type": "ListView",
      "padding": {"left": 0, "top": 0, "right": 0, "bottom": 16},
      "children": [
        {
          "type": "Container",
          "color": {"_color": 4288423856},
          "padding": {"left": 24, "top": 32, "right": 24, "bottom": 48},
          "child": {
            "type": "Column",
            "children": [
              {
                "type": "CircleAvatar",
                "radius": 50,
                "backgroundColor": {"_color": 4294967295},
                "child": {"type": "Icon", "icon": {"codePoint": 59389}, "size": 50, "color": {"_color": 4288423856}}
              },
              {"type": "SizedBox", "height": 16},
              {"type": "Text", "data": "{{name}}", "fontSize": 28, "fontWeight": 6, "color": {"_color": 4294967295}},
              {"type": "Text", "data": "{{role}}", "fontSize": 16, "color": {"_color": 2164260863}},
              {"type": "SizedBox", "height": 24},
              {
                "type": "Row",
                "mainAxisAlignment": 2,
                "children": [
                  {
                    "type": "Column",
                    "children": [
                      {"type": "Text", "data": "{{followers}}", "fontSize": 24, "fontWeight": 6, "color": {"_color": 4294967295}},
                      {"type": "Text", "data": "Followers", "fontSize": 12, "color": {"_color": 2164260863}}
                    ]
                  },
                  {"type": "SizedBox", "width": 48},
                  {
                    "type": "Column",
                    "children": [
                      {"type": "Text", "data": "{{following}}", "fontSize": 24, "fontWeight": 6, "color": {"_color": 4294967295}},
                      {"type": "Text", "data": "Following", "fontSize": 12, "color": {"_color": 2164260863}}
                    ]
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "Padding",
          "padding": {"left": 16, "top": 16, "right": 16, "bottom": 16},
          "child": {
            "type": "Row",
            "children": [
              {
                "type": "Expanded",
                "child": {
                  "type": "ElevatedButton",
                  "onPressed": {"_action": "follow"},
                  "backgroundColor": {"_color": 4288423856},
                  "child": {"type": "Text", "data": "Follow", "color": {"_color": 4294967295}}
                }
              },
              {"type": "SizedBox", "width": 12},
              {
                "type": "Expanded",
                "child": {
                  "type": "ElevatedButton",
                  "onPressed": {"_action": "message"},
                  "backgroundColor": {"_color": 2164260863},
                  "child": {"type": "Text", "data": "Message"}
                }
              }
            ]
          }
        },
        {"type": "Divider"},
        {
          "type": "ListTile",
          "leading": {"type": "Icon", "icon": {"codePoint": 57534}, "color": {"_color": 4288585374}},
          "title": {"type": "Text", "data": "Email"},
          "subtitle": {"type": "Text", "data": "{{email}}", "color": {"_color": 4288585374}}
        },
        {
          "type": "ListTile",
          "leading": {"type": "Icon", "icon": {"codePoint": 58719}, "color": {"_color": 4288585374}},
          "title": {"type": "Text", "data": "Location"},
          "subtitle": {"type": "Text", "data": "{{location}}", "color": {"_color": 4288585374}}
        },
        {"type": "Divider"},
        {
          "type": "Padding",
          "padding": {"left": 16, "top": 8, "right": 16, "bottom": 8},
          "child": {
            "type": "Text",
            "data": "Skills",
            "fontSize": 18,
            "fontWeight": 6
          }
        },
        {
          "type": "Padding",
          "padding": {"left": 16, "top": 0, "right": 16, "bottom": 8},
          "child": {
            "type": "Wrap",
            "spacing": 8,
            "children": [
              {"type": "Chip", "label": {"type": "Text", "data": "Flutter"}, "backgroundColor": {"_color": 2164260863}},
              {"type": "Chip", "label": {"type": "Text", "data": "Dart"}, "backgroundColor": {"_color": 2164260863}},
              {"type": "Chip", "label": {"type": "Text", "data": "UI/UX"}, "backgroundColor": {"_color": 2164260863}},
              {"type": "Chip", "label": {"type": "Text", "data": "Mobile"}, "backgroundColor": {"_color": 2164260863}}
            ]
          }
        }
      ]
    }
  }
}
